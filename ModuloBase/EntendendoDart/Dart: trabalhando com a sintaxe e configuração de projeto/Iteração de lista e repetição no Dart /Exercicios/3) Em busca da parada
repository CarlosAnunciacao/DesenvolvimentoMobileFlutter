import 'dart:io';

void main() {
  String frase = "Parou! Este código não continua.";

  for (int i = 0; i < frase.length; i++) {
    String letra = frase[i];

    if (letra == '!') {
      stdout.write(letra); // Imprime o '!' antes de parar
      break; // Interrompe o laço for imediatamente
    }

    stdout.write(letra);
  }
  
  print(''); 
}
