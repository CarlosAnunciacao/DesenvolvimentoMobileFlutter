void enviarEmail(String email) {
  print('mensagem enviada para: $email');
}

void main() {
  // c√≥digo comentado

  Map<String, String> pessoasEmails = {
    'joao': 'joao@gmail.com',
    'maria': 'maria@gmail.com',
  };

  pessoasEmails['enzo'] = 'enzo@gmail.com';

  for (String nome in pessoasEmails.keys) {
    print(nome);
  }
}
